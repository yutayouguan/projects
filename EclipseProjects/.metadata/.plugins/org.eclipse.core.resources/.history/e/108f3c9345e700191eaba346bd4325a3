package mi.yike.thread;

/**
 * @author wangmeng
 *join 方法
 */

class mom extends Thread{
public mom(String name) {
	super(name);
	// TODO Auto-generated constructor stub
}
	@Override
		public void run() {
		System.out.println("妈妈注备做饭！");
		System.out.println("妈妈煮饭！");
		System.out.println("妈妈洗菜！");
		System.out.println("妈妈炒菜！");//发现酱油没有了，叫儿子去买酱油
		
		System.out.println("妈妈炒好菜了！");
		System.out.println("一家人开心的在饭桌上享受晚饭！");
		}
	
}

class son extends Thread{
	public son(String name) {
		super(name);
		// TODO Auto-generated constructor stub
	}
	@Override
	public void run() {
		System.out.println(this.getName()+"下楼");
	}
	
}
public class Demo4 {
public static void main(String[] args) {
	
}
}
