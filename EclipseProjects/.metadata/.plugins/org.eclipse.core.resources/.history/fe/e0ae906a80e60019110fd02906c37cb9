package ml.yike.thread;


class TicketSale extends Thread{
	static int num = 50;

	
	public TicketSale(String nameString)
	{
		super(nameString);		
	}
	
	@Override
	public void run() {
		if (num>0) {
			System.out.println();
		}

	}
	
	
	
}

